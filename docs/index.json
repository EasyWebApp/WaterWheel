    "content": "{\n  \"name\": \"waterwheel\",\n  \"version\": \"0.2.0\",\n  \"description\": \"Modern HTTP client based on AsyncGenerator function in ES 9\",\n  \"keywords\": [\n    \"HTTP\",\n    \"client\",\n    \"async\",\n    \"generator\",\n    \"function\",\n    \"iterator\"\n  ],\n  \"author\": \"shiy2008@gmail.com\",\n  \"license\": \"LGPL-3.0\",\n  \"homepage\": \"https://easywebapp.github.io/WaterWheel/\",\n  \"repository\": {\n    \"type\": \"git\",\n    \"url\": \"git+https://github.com/EasyWebApp/WaterWheel.git\"\n  },\n  \"bugs\": {\n    \"url\": \"https://github.com/EasyWebApp/WaterWheel/issues\"\n  },\n  \"main\": \"source/index.js\",\n  \"browser\": \"dist/WaterWheel.js\",\n  \"scripts\": {\n    \"bundle\": \"babel source/ -d dist/  &&  amd-bundle dist/index dist/WaterWheel\",\n    \"build\": \"npm run bundle  &&  esdoc  &&  git add .\",\n    \"start\": \"web-server docs/ -o\",\n    \"preview\": \"web-server -o test/\",\n    \"check\": \"cross-env NODE_ENV=test  mocha\",\n    \"test\": \"npm run check -- --exit\",\n    \"debug\": \"npm run check -- --inspect-brk\"\n  },\n  \"dependencies\": {\n    \"babel-polyfill\": \"^6.26.0\",\n    \"babel-runtime\": \"^6.26.0\"\n  },\n  \"devDependencies\": {\n    \"amd-bundle\": \"^1.3.2\",\n    \"babel-cli\": \"^6.26.0\",\n    \"babel-plugin-transform-async-generator-functions\": \"^6.24.1\",\n    \"babel-preset-env\": \"^1.7.0\",\n    \"babel-register\": \"^6.26.0\",\n    \"cross-env\": \"^5.2.0\",\n    \"esdoc\": \"^1.1.0\",\n    \"esdoc-ecmascript-proposal-plugin\": \"^1.0.0\",\n    \"esdoc-external-webapi-plugin\": \"^1.0.0\",\n    \"esdoc-standard-plugin\": \"^1.0.0\",\n    \"koapache\": \"^1.0.2\",\n    \"mocha\": \"^5.2.0\",\n    \"should\": \"^13.2.3\",\n    \"should-sinon\": \"0.0.6\",\n    \"sinon\": \"^6.1.4\"\n  },\n  \"babel\": {\n    \"plugins\": [\n      \"transform-async-generator-functions\"\n    ],\n    \"env\": {\n      \"test\": {\n        \"presets\": [\n          \"env\"\n        ],\n   