{
  "name": "waterwheel",
  "version": "0.2.0",
  "description": "Modern HTTP client based on AsyncGenerator function in ES 9",
  "keywords": [
    "HTTP",
    "client",
    "async",
    "generator",
    "function",
    "iterator"
  ],
  "author": "shiy2008@gmail.com",
  "license": "LGPL-3.0",
  "homepage": "https://github.com/EasyWebApp/WaterWheel#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EasyWebApp/WaterWheel.git"
  },
  "bugs": {
    "url": "https://github.com/EasyWebApp/WaterWheel/issues"
  },
  "main": "source/index.js",
  "browser": "dist/WaterWheel.js",
  "scripts": {
    "build": "babel source/ -d dist/  &&  amd-bundle dist/index dist/WaterWheel",
    "start": "web-server -o test/",
    "check": "cross-env NODE_ENV=test  mocha",
    "test": "npm run check -- --exit",
    "debug": "npm run check -- --inspect-brk"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0"
  },
  "devDependencies": {
    "amd-bundle": "^1.3.2",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "cross-env": "^5.2.0",
    "koapache": "^1.0.2",
    "mocha": "^5.2.0",
    "should": "^13.2.3",
    "should-sinon": "0.0.6",
    "sinon": "^6.1.4"
  },
  "babel": {
    "plugins": [
      "transform-async-generator-functions"
    ],
    "env": {
      "test": {
        "presets": [
            "env"
        ],
        "plugins": [
            "transform-async-generator-functions"
        ]
      }
    }
  }
}
